	<queryString>
		<![CDATA[SELECT invoice_no, invoice_date, sum(cgst), sum(sgst), 
sum(total_gst_amt), sum(total_amt)
FROM purchase
WHERE invoice_date BETWEEN
$P{from} AND $P{to} 
GROUP by invoice_no

UNION

SELECT invoice_no, invoice_date, sum(cgst), sum(sgst), 
sum(total_gst_amt), sum(total_amt)
FROM billing
WHERE invoice_date BETWEEN
$P{from} AND $P{to}
GROUP by invoice_no]]>
	</queryString>
